  "version": "0.0.6",
  "dependencies": {
    "@fiahfy/semaphore": "^0.0.2",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@crxjs/vite-plugin": "^2.0.0-beta.26",
    "@fiahfy/biome-config": "^0.0.1",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/chrome": "^0.0.278",
    "husky": "^9.1.6",
